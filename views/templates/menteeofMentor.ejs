<!-- ======= alumni Section ======= -->
<section id="alumni" class="alumni section-bg">
    <div class="container">

      <div class="section-title">
        <h2>Mentee Details </h2>
    </p>
      </div>



      <div class="row">
        <div class="col-lg-4">
          <img src="assets/img/testimonials/testimonials-2.jpg" class="img-fluid" alt="">
          
        </div>
        <div class="col-lg-8 pt-4 pt-lg-0 content">
          <% if(!mentorData){ %>
            <h5>No Mentee assigned</h5>
          <% } else {%>
          <h5>Mentee</h5>
          <h3><%= mentorData['name'] %></h3>
          <div class="row">
            <div class="col-lg-6">
              <ul>
                <li><i class="bi bi-chevron-right"></i> <strong>Email:</strong> <span><%= mentorData['email'] %></span></li>
                <li><i class="bi bi-chevron-right"></i> <strong>Phone No:</strong> <span><%= mentorData['phone'] %></span></li>
                <li><i class="bi bi-chevron-right"></i> <strong>Qualification:</strong> <span><%= mentorData['qualification'] %></span></li>
              </ul>
            </div>
            <div class="col-lg-6">
              <ul>
                <li><i class="bi bi-chevron-right"></i> <strong>Age Group</strong>:</strong> <span><%= mentorData['ageGroup'] %></span></li>
                <li><i class="bi bi-chevron-right"></i> <strong>Designation:</strong> <span><%= mentorData['designation'] %></span></li>
                
              </ul>
            </div>
          </div>
        </div>
      </div>
<% } %>
     

      

    </div>
    
  </section><!-- End alumni Section -->
   <!-- ======= Footer ======= -->
   <footer id="footer">
    <div class="container footer-bottom clearfix">
      
      <div class="credits">
